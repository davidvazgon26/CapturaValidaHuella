<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<runtime>
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<probing privatePath="lib"/>
			<!-- Buscar DLLs en /lib -->
		</assemblyBinding>
	</runtime>
	<appSettings>
		<add key="Port" value="8080"/>
	</appSettings>
</configuration>